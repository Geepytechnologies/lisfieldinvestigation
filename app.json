{
  "expo": {
    "name": "fieldinvestigation",
    "slug": "fieldinvestigation",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/stateicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyB5hd3Vq9nKM_oqi1rdV-mxKtv7IL_EEfU"
      },
      "bundleIdentifier": "com.geepytech.fieldinvestigation"
    },
    "android": {
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "myapp"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB5hd3Vq9nKM_oqi1rdV-mxKtv7IL_EEfU"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/stateicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.geepytech.fieldinvestigation",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/stateicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/stateicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "785c01bf-8ab5-4ea7-bd9c-b16d2af2ff5d"
      }
    },
    "owner": "geepytech"
  }
}
